{
  "name": "nephele-serve",
  "version": "1.0.0-alpha.34",
  "description": "Command line WebDAV server with browser support for local users to access files remotely.",
  "type": "module",
  "bin": {
    "nephele-serve": "nephele-serve.cjs"
  },
  "scripts": {
    "clean": "test -d bin && rm -r bin || true",
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nephele.git"
  },
  "keywords": [
    "webdav",
    "dav",
    "server",
    "serve",
    "file server",
    "webdav server",
    "cli",
    "command line",
    "terminal",
    "command",
    "personal cloud",
    "local file server",
    "file share",
    "nas",
    "network storage",
    "nephele"
  ],
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sciactive/nephele/issues"
  },
  "homepage": "https://github.com/sciactive/nephele#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/update-notifier": "^6.0.5",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@nephele/adapter-file-system": "^1.0.0-alpha.34",
    "@nephele/adapter-virtual": "^1.0.0-alpha.34",
    "@nephele/authenticator-htpasswd": "^1.0.0-alpha.34",
    "@nephele/authenticator-none": "^1.0.0-alpha.34",
    "@nephele/plugin-index": "^1.0.0-alpha.34",
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "nephele": "^1.0.0-alpha.34",
    "update-notifier": "^6.0.2"
  },
  "optionalDependencies": {
    "@nephele/authenticator-pam": "^1.0.0-alpha.34",
    "userhomepath": "^1.0.6",
    "userid": "^1.2.5"
  },
  "engines": {
    "node": ">=18"
  }
}
