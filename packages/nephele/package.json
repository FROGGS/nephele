{
  "name": "nephele",
  "version": "1.0.0-alpha.3",
  "description": "A pluggable WebDAV, CardDAV, and CalDAV server for Node.js and Express.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "test -d dist && rm -r dist || true",
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/nephele.git"
  },
  "keywords": [
    "webdav",
    "carddav",
    "caldav",
    "dav",
    "server",
    "express",
    "middleware"
  ],
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sciactive/nephele/issues"
  },
  "homepage": "https://github.com/sciactive/nephele#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/content-type": "^1.1.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/uuid": "^8.3.4",
    "@types/vary": "^1.1.0",
    "@types/xml2js": "^0.4.11",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@sciactive/splitn": "^2.0.0",
    "content-type": "^1.0.4",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "nanoid": "^4.0.0",
    "range-parser": "^1.2.1",
    "uuid": "^8.3.2",
    "vary": "^1.1.2",
    "xml2js": "^0.4.23"
  },
  "engines": {
    "node": ">=16"
  }
}
